@CHARSET "UTF-8";

a,a:hover{
	text-decoration: none;
	color:inherit;
}

.allitem{
	background:#f5f5f5;
	width:350px;
	position:fixed;
	overflow:scroll;
	overflow-x:hidden;
	left:0;
}
.allitem p{
	font-weight:bolder;
	font-size:35px;
	margin-left:92px;
	margin-top:20px;
	margin-bottom:20px;
}
::-webkit-scrollbar{
	width: 10px;
}
/* 垂直滚动条的滑动块 */
::-webkit-scrollbar-thumb:vertical {
	border-radius: 4px;
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
	background-color:rgba(129,129,129,0.2);
}
.mathtab{
	margin-top:10px;
	height:30px;
	width:300px;
	margin-left: 30px;
}
.mathtab li{
	list-style:none;
	float:left;
	width:130px;
	height:30px;
	text-align:center;
	font-weight:bold;
	line-height:30px;
	background:#f90;
}
.mathtab li a{
	color:white!important;
}
.firstitem{
	width:300px;
	height:auto;
	margin-left: 30px;
}
.firstitem ul{
    margin-left: 15px;
	overflow: hidden;
	width: 280px;
	font-size: 16px;
    color: #000;
}
.items{
	background-repeat:repeat-x;
	margin-top: 5px;
	line-height: 37px;
	letter-spacing: 3px;
	height: 35px;
	background-size: 15px;
	cursor:pointer;
	margin-left: 20px;
	overflow:hidden;
	width: 240px;
}
.items2{
	width:250px;
	background-size:20px;
	height:25px;
	margin-top:30px;
	font-weight:bold;
	cursor:pointer;
}
.icon{
	float:left;
	padding:5px 8px;
	display:none;
}
.icon2{
	float:left;
	padding:9px 8px;
	display:none;
}

.shuati{
	min-height: 600px;
	max-height:auto;
	border: 1px solid #f90;
	border-radius: 5px;
	padding: 20px 20px;
	background:#fff;
	z-index: 90;
}
.buttons{
	background:#74c409;
	color:#fff;
	margin-left:65px;
	border:0;
	padding:5px;
	width:80px;
}
.btn:active,.btn:focus{
	color:#fff!important;
}

.cuoti{
	width:300px;
	position:fixed;
	right:20px;
	top:130px;
	height: 500px;
	border: 1px solid #f90;
	border-radius: 9px ;
	background-color: white;
	margin-top:15px;
}
.h4{
     background-color: orange;
     color: white;
     text-align: center;
     height: 50px;
     border-radius: 9px;
     line-height: 48px;
     margin: 0;
}
.span1{
	color:orange;
	font-size: large;
}


.rem{
	width:1px; 
	height:600px;
	position: fixed;
	right:5px;
	top:120px;
	border:1px solid #ddd;
	z-index:99;
	background:#fff;
	display: none;
}
.tab{
	width:320px;
	height:60px;
	margin-bottom:30px;
}
.tab span{
	width:50%;
	height:60px;
	display:block;
	float:left;
	text-align:center;
	font-size:18px;
	font-weight:400;
	background:#f90;
	cursor:pointer;
	color: #fff;
	line-height:3.2em;
	letter-spacing: 3px;
	opacity: 0.8;
}
.tab1{
	color:#000!important;
	background:#fff!important;
	opacity: 1;
}
.com{
	width:300px;
	height:auto;
	border-top:1px solid #ddd;
	padding:15px;
}
.com-show{
	width:100%;
	height:500px
	overflow:auto;
}
.com-show ul li{
	padding: 20px 15px 0 5px;
	color:#333;
	border-bottom:1px solid #ddd;
}
.com-show ul li p{
	text-indent:2em;
    font-weight:normal;
    font-size:15px;
}
.showtime{
	float:right;
	font-size:12px;
    color:#333;
    font-weight:normal;
}
.com-write{
	width:320px;
	height:auto;
	padding:15px;
	display:none;
}
.submit{
	border:0;
	padding:8px 10px;
	background:#f90;
	color:#fff;
	margin-top:8px;
	float:right;
	margin-bottom:20px;
}
.close{
	width:60px;
	height:60px;
	position:absolute;
	right:286px;
	top:300px;
	border-radius: 50%;
	background:#ddd;
	opacity:0.4;
}
.rightImg{
	margin-top:22px;
	margin-left:25px;
}
.close:hover{
	opacity:0.8;
}

.rem2{
	width:70px;
	height:70px;
	background:#f90;
	position:fixed;
	right:5px;
	top:300px;
	cursor:pointer;
	opacity:0.6;
	text-align:center;
	border-radius:50%;
}
.rem2:hover{
	opacity:1;
}
.rem2 span{
	color:#fff;
	font-size:15px;
	line-height:4.7em;
}
.ser{
	width:450px;
	height:50px;
	text-align:center;
	border-radius: 4px;
	margin:25px auto;
	background:linear-gradient(#f0f0f0,#ddd);
	cursor:pointer;
    line-height: 50px;
}
.ser img{
	vertical-align: middle;
}
.ser span{
	margin-left: 10px;
	font-family: "微软雅黑";
	font-weight: bold;
	letter-spacing: 3px;
}
.search{
	width:400px;
	height:50px;
	float:left;
	display:none;
	border:0px;
	background:#f0f0f0;
	border-radius: 4px;
	text-align: center;
	font-size:15px;
	letter-spacing: 3px;
	color:#000;
}
.success{
	width:150px;
	hegiht:55px;
	text-align: center;
	line-height: 55px;
	border-radius: 5px;
	font-size:20px;
	letter-spacing: 3px;
	background:#696d87;
	color:#fff;
	border:1px solid #ddd;
	display: none;
}

/* =================错题排行榜=============== */
.cuoti-list > li{
	list-style: none;
	line-height: 2em;
}
.cuoti-list li >p{
	display: inline;
	font-size: 15px;
}
.cuoti-list li > img{
	display: inline;
	width: 20px;
	height: 20px;
}
.cuoti-list li > span{
	font-size: 20px;
	padding: 12px;
}
.houmian{
	color: gray;
}

.null-img{
	margin-top: 160px;
	margin-left: 100px;
	position: absolute;
}
.null-tishi{
	margin-left: -60px;
	color: #bfbfbf;
}

