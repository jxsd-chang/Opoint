@charset "utf-8";
.leftChoose{
	width:255px;
	height:615px;
	float:left;
	padding:40px 25px;
	margin-left: 30px;
	text-align: center;
}
.leftChoose h3{
	background:linear-gradient(#f0f0f0,#ddd);
	display: block;
	width:100%;
	height:40px;
	line-height: 40px;
	border-radius: 4px;
	letter-spacing: 3px;
}
.collect,.setting,.info{
	border-top:1px solid #ddd;
	margin-top:20px;
}

.setting li,.collect li,.info li{
	width:100%;
	height:50px;
	line-height: 50px;
	text-align: center;
	cursor:pointer;
	letter-spacing: 3px;
}

.act{
	background: rgba(240,20,20,.8);
    border-radius: 8px 0 0 8px;
	color:#fff;
	position: relative;
}
.act::after{
	position: absolute;
    border-top: 26px solid #fff;
    border-bottom: 26px solid #fff;
	display: block;
    content: "";
    bottom: 0;
    right: 0;
    border-left: 18px solid transparent;
}
.count{
	display: inline-block;
	width:25px;
	height:25px;
	border-radius: 50%;
	margin-left:15px;
	background:#f90;
	color:#fff;
	margin-top: 10px;
    position: absolute;
    font-size:12px;
    font-weight:bold;
    z-index:6;
    line-height: 25px;
    text-align: center;
    padding-left: 3px;
}
