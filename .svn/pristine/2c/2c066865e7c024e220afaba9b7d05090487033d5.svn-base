@CHARSET "UTF-8";

.leftChoose{
	width:255px;
	height:350px;
	float:left;
	padding:40px 25px;
	text-align: center;
}
.leftChoose h3{
	background:linear-gradient(#f0f0f0,#ddd);
	display: block;
	width:100%;
	height:40px;
	line-height: 40px;
	border-radius: 4px;
	letter-spacing: 3px;
}
.collect{
	border-top:1px solid #ddd;
	margin-top:20px;
}
.collect li{
	width:100%;
	height:50px;
	line-height: 50px;
	text-align: center;
	cursor:pointer;
	letter-spacing: 3px;
}
.num{
	display:block;
	width:25px;
	height:25px;
	border-radius: 50%;
	background:#ddd;
	color:red;
	font-weight:bold;
	text-align: center;
	float: right;
	position: relative;
	z-index: 10;
	margin-top: -38px;
	margin-right: 30px;
	line-height: 25px;
}
.act{
	background: rgba(240,20,20,.8);
    border-radius: 8px 0 0 8px;
	color:#fff;
	position: relative;
}
.act::after{
	position: absolute;
    border-top: 26px solid #fff;
    border-bottom: 26px solid #fff;
	display: block;
    content: "";
    bottom: 0;
    right: 0;
    border-left: 18px solid transparent;
}
.contain-right2{
	width:790px;
	min-height:600px;
	box-sizing:border-box;
	float:right;
	padding-top:25px;
	border-radius:3px;
	background:#fff;
	border:1px solid #ddd;
}
.contain-right{
	width:790px;
	min-height:600px;
	box-sizing:border-box;
	margin:0 auto;
	padding-top:25px;
	border-radius:3px;
	background:#fff;
	border:1px solid #ddd;}

.person{
	font-size:25px;
	font-weight:bold;
	letter-spacing:5px;
	margin:20px auto;
	display: block;
	width:125px;}

.changePic{
	width:545px;
	height:400px;
	margin-top:25px;
	margin-left:90px;}

.tip{
	width:150px;
	height:40px;}

.tip p{
	font-weight:bold;
	font-size:20px;
	letter-spacing:3px;}

.tip hr{
	width:100%;
	background:#f90;
	border:0;
	margin-top:-20px;}


.fileinput{
	float:left;
	position:relative; 
	display: inline-block; 
	overflow: hidden;
	margin-left: 30px;
    margin-top: 15px;}

.fileinput span{
    background: #f90;
    cursor: pointer;
    opacity:0.8;
    color: aliceblue;
    padding: 10px 20px;
    border-radius: 5px;
    display: block;
    font-size: 14px;}
	
.fileinput input{
	position: absolute;
	cursor:pointer;  
	left: 0px;
	top:0px; 
	opacity: 0; 
	filter: 'alpha(opacity=0)';
	height:40px;}


canvas {
	border: solid thin #ccc;
	float:left;}

#canvasContainer {
    width: 370px;
    height: 190px;
	position:relative;
	float: right;}

.save{
	background: #f90;
    cursor: pointer;
    opacity:0.8;
    color: aliceblue;
    padding: 10px 20px;
    border-radius: 5px;
    display: block;
    font-size: 14px;
    border:0;
    float:right;
    margin-top:180px;
}



.changePwd{
	margin-top:15px;
	margin-left:90px;
	width:545px;
	height:380px;}

.pwdinput{
	width:400px;
	height:50px;
	margin-top:30px;}

.pwdinput span{
	margin-left:30px;
	margin-top:10px;}

.pwd{
	width:200px;
	height:40px;
	margin-left:20px;
	border:1px solid #666;
	letter-spacing:3px;
	border-radius:4px;
	padding-left:20px;}
.wrong{
	font-size:13px;
	color:red;
	margin-left: 120px;
	opacity: 0;
}
.submit-btn{
	float:right;
	padding:8px 15px;
	background:#333;
	opacity:0.8;
	color:#fff;
	border-radius:4px;
	border:0;
	cursor:pointer;}

.classList{
	width:1000px;
	margin:0 auto;
}
	
#newclass{
	margin:25px 100px;
	color:#145f7a;
	cursor:pointer;
	font-size: 17px;
}
.classList p:hover{
	color:red;
}
.classTable{
	margin:20px auto;
	width:80%;
	text-align: center;
}
.classTable thead{
	background:#ddd;
	height:45px;
	letter-spacing: 3px;
}
.classTable td{
	border-left:2px solid #848484;
	height:40px;
}
.classTable tbody>tr td:first-child{
	border-left:1px solid #ddd;
}
.classTable tbody>tr>td:last-child{
	border-right:1px solid #ddd;
}
.classTable td span{
	cursor:pointer;
	color:#333;
}
.classTable tbody>tr:nth-of-type(even){
	background:#f0f0f0;
}
.classTable td span:hover{
	color:red;
}
.stulist{
	position: absolute;
	right:5px;
	top:130px;
	width:250px;
	height:500px;
	border:1px solid #ddd;
	background:#fff;
	z-index: 7;	
	display: none;
}
.hid{
	width:60px;
	height:60px;
	position:absolute;
	cursor:pointer;
	right:218px;
	top:240px;
	border-radius: 50%;
	background:#ddd;
	opacity:0.4;
}
.rightImg{
	margin-top:22px;
	margin-left:25px;
}
.close:hover{
	opacity:0.8;
}
.list2{
	font-size:18px;
	font-weight:400px;
	text-align: center;
	margin:20px;
}
.list3{
	width:180px;
	margin:0 auto;
	list-style: none;
}
.list3 li{
	height:60px;
	padding: 10px 0px 10px 0px;
	font-size:14px;
	line-height: 25px;
	text-align: left;
	border-top: 2px dashed #ddd;
}
.list4{
	font-size:12px;
	color:#a5a5a5;
}
.list5{
	font-size:12px;
	color:#a5a5a5;
	margin-left:15px;
}
.stuimg{
	width:35px;
	height:35px;
	border-radius: 50%;
	float:left;
	margin-right:5px;
	vertical-align: middle;
	margin-top: 10px;
}
.stu-xinxi{
	float: right;
	width: 130px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.can{
	display: none; 
	background:black; 
	opacity: 0.5; 
	width:100%; 
	height: 100%;  
	position: fixed; 
	z-index:7;
	top:0px; 
	left:0px;}

.message-config{
	z-index:9;
	padding: 20px;
	border: 1px solid #FFCECF; 
	border-radius:7px;
	margin: 0 auto; 
	display: none;}

.message-config-head{
	width:100%; 
	height: 30px;}
	
.message-config-head span{
	line-height: 30px; 
	padding-left: 30px; 
	font-size: 20px; }
	 
.message-config-head img{
	float:right;
	cursor:pointer;}

.in-line{
	width:480px;
	margin:20px auto;}

.in-line ul{
	width:450px;
	margin: 0 auto;
	list-style: none;}

.in-line ul li{
	min-height:40px;
	margin-bottom: 10px;
}

.queren{
	font-size:18px;
	letter-spacing: 3px;
}
.del{
	width:67px;
	height:30px;
	line-height:30px;
	border:0px;
	background:#f90;
	cursor:pointer;
	color:#fff;
	border-radius: 4px;
	text-align: center;
	float:left;
	margin-left:50px;
}

.aa{
	margin:25px 50px;
	font-size: 17px;
}
.fujian{
	float:right;
	margin-right:15px;
	cursor:pointer;
	font-size: 14px;
	color:#ff3131;
}
.fujian:hover{
	color:red;
}
.question{
	width:550px;
	margin:10px 15px;
	border:1px solid #ddd;
	padding-bottom: 10px;
}
.bb{
	padding:10px 20px;
	background:#f0f0f0;
}
.cc{
	padding:15px 15px;
	text-indent: 2em;
}
.ans,.tijiao{
	padding:5px 10px;
	background:#f90;
	color:#fff;
	border:none;
	float:right;
	border-radius:5px;
	cursor:pointer;
}
.text{
	width:235px;
	min-height:160px;
	margin-left:10px;
	float:left;
	margin-top:10px;
	opacity: 0;
}

.infoInput{
	width:180px;
	height:35px;
	padding-left:5px;
	border-radius: 5px;
	border:2px solid #b0b0b0;
}
.textarea{
	margin-left: 80px;
	border-radius: 5px;
	border: 2px solid #b0b0b0;
	text-indent: 2em;
	padding:5px 5px;
	width:300px;
	resize: none;
	min-height: 150px;
	max-height:250px;
}
.submitBtn{
	width:100px;
	height:35px;
	line-height: 35px;
	margin:0 auto;
	background:#f90;
	color:#fff;
	border:0;
	border-radius: 5px;
	display: block;
	margin-top: 15px;
}
.noquestion{
	font-size:24px;
	width: 300px;
	margin: 10px auto;
	text-align: center;
}
.no2{
	font-size:15px;
	color:#979797;
}


.non-stu-list{
	padding-left: 55px;
	margin-top: 115px;
}
.non-stu-tishi{
	display: block;
	color: #cdcdcd;
	margin-left: -40px;
}

