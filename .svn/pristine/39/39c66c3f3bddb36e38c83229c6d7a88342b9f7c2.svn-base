@CHARSET "UTF-8";
.contact-us{
	width: 100%;
}
.contact-container{
	width: 1200px;
	margin: 0 auto;
	/* padding: 30px 30px 70px 30px; */
}
.con-address{
	height: 360px;
	padding: 30px;
	
}
.address{
	float: left;
	width:580px;
	height: 300px;
	background-color: #f7f7f7;
}
.address-img{
	float: right;
	width: 500px;
	height: 300px;
}
.address h2{
	height: 26px;
	line-height: 26px;
	margin-top: 50px;
	margin-bottom: 30px;
}
.address p,.kefu p{
	line-height: 30px;
}
.kefu{
	width: 580px;
	height: 300px;
	float: left;
	padding-left: 30px;
}
.fankui{
	width: 500px;
	height: 300px;
	float: right;
	
}
.kefu h2,.fankui h2{
	margin-bottom: 20px;
}

.contact-service{
	height: 300px;
	padding: 0 30px;
}
span{
	font-weight: bold;
}
.jianyi:HOVER{
	border-color: #F90;
}


