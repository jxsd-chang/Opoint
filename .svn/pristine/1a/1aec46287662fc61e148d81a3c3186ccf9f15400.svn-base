.fileinput{
	float:left;
	position:relative; 
	display: inline-block; 
	overflow: hidden;
	margin-left: 30px;
    margin-top: 60px;}

.fileinput span{
    background: #f90;
    cursor: pointer;
    opacity:0.8;
    color: aliceblue;
    padding: 10px 20px;
    border-radius: 5px;
    display: block;
    font-size: 14px;}
	
.fileinput input{
	position: absolute;
	cursor:pointer;  
	left: 0px;
	top:0px; 
	opacity: 0; 
	filter: 'alpha(opacity=0)';
	height:40px;}


canvas {
